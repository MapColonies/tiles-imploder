{
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "tilesDirectoryPath": "/path/to/tiles",
  "queue": {
    "jobManajerBaseUrl": "http://localhost:8082",
    "heartbeatManagerBaseUrl": "http://localhost:8083",
    "dequeueIntervalMs": 5000,
    "heartbeatIntervalMs": 5000,
    "jobType": "rasterTilesExporter",
    "taskType": "rasterTilesExporter"
  },
  "maxAttempts": 3,
  "geohash": {
    "precision": 7
  },
  "gpkg": {
    "path": "/path/to/gpkg",
    "resampling": "bilinear",
    "tilingScheme": "InspireCRS84Quad"
  },
  "batch": {
    "size": 15
  },
  "httpRetry": {
    "attempts": 5,
    "delay": "exponential",
    "shouldResetTimeout": true
  }
}
