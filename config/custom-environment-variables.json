{
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "enabled": "TELEMETRY_TRACING_ENABLED",
      "url": "TELEMETRY_TRACING_URL"
    },
    "metrics": {
      "enabled": "TELEMETRY_METRICS_ENABLED",
      "url": "TELEMETRY_METRICS_URL",
      "interval": "TELEMETRY_METRICS_INTERVAL"
    }
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "tilesDirectoryPath": "TILES_DIR_PATH",
  "queue": {
    "jobManagerBaseUrl": "QUEUE_JOB_MANAGER_BASE_URL",
    "heartbeatManagerBaseUrl": "QUEUE_HEARTBEAT_MANAGER_BASE_URL",
    "dequeueIntervalMs": "QUEUE_DEQUEUE_INTERVAL_MS",
    "heartbeatIntervalMs": "QUEUE_HEARTBEAT_INTERVAL_MS",
    "jobType": "QUEUE_JOB_TYPE",
    "taskType": "QUEUE_TASK_TYPE"
  },
  "maxAttempts": {
    "__name": "MAX_ATTEMPTS",
    "__format": "number"
  },
  "geohash": {
    "precision": {
      "__name": "GEOHASH_PRECISION",
      "__format": "number"
    }
  },
  "gpkg": {
    "path": "GPKG_PATH",
    "resampling": "GPKG_RESAMPLING",
    "tilingScheme": "GPKG_TILING_SCHEME"
  },
  "batch": {
    "__name": "BATCH_SIZE",
    "__format": "number"
  }
}
