{
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "tilesDirectoryPath": "/etc/tiles",
  "downloadServerUrl": "http://localhost:8084/downloads",
  "gpkg": {
    "intermediatePath": "/etc",
    "finalPath": "/tmp",
    "resampling": "average",
    "tilingScheme": "InspireCRS84Quad"
  },
  "batchSize": 15
}
